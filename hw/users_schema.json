{
    "$schema": "http://json-schema.org/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "name": {
                "type": "string"
            },
            "gender": {
                "type": "string"
            },
            "address": {
                "type": "string"
            },
            "age": {
                "type": "integer"
            },
            "books": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "title": {
                            "type": "string"
                        },
                        "author": {
                            "type": "string"
                        },
                        "pages": {
                            "type": "integer"
                        },
                        "genre": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "author",
                        "genre",
                        "pages",
                        "title"
                    ]
                }
            }
        },
        "required": [
            "address",
            "age",
            "books",
            "gender",
            "name"
        ]
    }
}